{
  "name": "vs-code-typewriter",
  "displayName": "VS Code TypeWriter",
  "description": "Generate & sync typescript classes from your C# code",
  "version": "0.0.1",
  "publisher": "stewart-r",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.showOutput",
        "title": "Start TypeWriter"
      }
    ]
  },
  "activationEvents": [
    "onCommand:extension.showOutput"
  ],
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "precompile":"./node_modules/.bin/copy  ../VsCodeTypewriter.WebApi/bin/Debug/netcoreapp1.1/*.* ./server/",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "copy": "^0.3.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
